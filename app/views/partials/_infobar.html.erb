      <span  id="rus_" class="lang-bars lang-act" title="Русский язык">RUS</span>
      <span  id="ua" class="lang-bars" title="Україньска мова">UA</span>
      <span  id="de" class="lang-bars" title="Deutsch language">DE</span>
      <% elsif cookies[:language] == "ukr"  %>
      <span  id="eng" class="lang-bars" title="English language">ENG</span>
      <span  id="rus" class="lang-bars" title="Русский язык">RUS</span>
      <span  id="ua_" class="lang-bars lang-act" title="Україньска мова">UA</span>
      <span  id="de" class="lang-bars" title="Deutsch language">DE</span>
      <% elsif cookies[:language] == "de"  %>
      <span  id="eng" class="lang-bars" title="English language">ENG</span>
      <span  id="rus" class="lang-bars" title="Русский язык">RUS</span>
      <span  id="ua" class="lang-bars" title="Україньска мова">UA</span>
      <span  id="de_" class="lang-bars lang-act" title="Deutsch language">DE</span>
      <% else %>
      <span  id="eng" class="lang-bars" title="English language">ENG</span>
      <span  id="rus_" class="lang-bars lang-act" title="Русский язык">RUS</span>
      <span  id="ua" class="lang-bars" title="Україньска мова">UA</span>
      <span  id="de" class="lang-bars" title="Deutsch language">DE</span>
      <% end %>
    </div>
  </div>
</div><!-- info bar -->